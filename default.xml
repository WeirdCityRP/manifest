<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="WeirdCityRP"
           fetch="https://github.com/WeirdCityRP/" />

  <default revision="alpha"
           remote="WeirdCityRP"
           sync-c="true"
           sync-j="4" />

  <project path="manifest" name="manifest" >
    <copyfile src="run.sh" dest="run.sh" />
  </project>

  <!-- Weird City Roleplay Resources -->
  <project path="server-data/resources/[weirdcityrp]/w-base" name="w-base" />
  <project path="server-data/resources/[weirdcityrp]/w-chat" name="w-chat" />
  <project path="server-data/resources/[weirdcityrp]/w-jobs" name="w-jobs" />
  <project path="server-data/resources/[weirdcityrp]/w-loading-screen" name="w-loading-screen" />
  <project path="server-data/resources/[weirdcityrp]/w-sync" name="w-sync" />

  <project path="server-data/resources/[weirdcityrp]/bob74_ipl" name="bob74_ipl" revision="master" />

  <!-- FiveM -->
  <!-- <project path="server" name="fivem_binaries_linux" revision="master" /> -->
  <project path="server-data" name="cfx-server-data" />

</manifest>